{
    "name": "typicalbot",
    "version": "4.0.0-canary.0",
    "description": "TypicalBot is far from typical. It's stable, lightning fast, and easy to useâ€” TypicalBot will seamlessly help you moderate your server and provide entertainment for its members, no problem at all!",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "private": true,
    "scripts": {
        "postinstall": "husky install",
        "build": "tsc",
        "build:clean": "git clean -d -x -e node_modules -e .env -f && tsc",
        "build:watch": "tsc --watch",
        "build:database": "node bin/setup.js",
        "build:instance": "tsc && node bin/pm2.js",
        "lint": "eslint packages --ext .ts",
        "lint:fix": "eslint packages --ext .ts --fix",
        "start": "tsc && node dist/packages/bot/src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typicalbot/typicalbot.git"
    },
    "keywords": [
        "discordjs",
        "javascript",
        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/typicalbot/typicalbot/issues"
    },
    "homepage": "https://github.com/typicalbot/typicalbot#readme",
    "dependencies": {
        "@sentry/node": "^6.2.2",
        "discord.js": "github:discordjs/discord.js#master",
        "dotenv": "^8.2.0",
        "fs": "0.0.2",
        "i18next": "^19.9.2",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^9.3.0",
        "moment": "^2.29.1",
        "node-fetch": "^2.6.1",
        "pm2": "^4.5.5",
        "rethinkdb-ts": "^2.4.9",
        "tslib": "^2.1.0",
        "veza": "^1.1.0",
        "vm2": "^3.9.2",
        "winston": "^3.3.3"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.3",
        "erlpack": "github:discord/erlpack",
        "utf-8-validate": "^5.0.4",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "@types/i18next-node-fs-backend": "^2.1.0",
        "@types/klaw": "^3.0.1",
        "@types/mathjs": "^6.0.11",
        "@types/node": "^14.14.33",
        "@types/node-fetch": "^2.5.8",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "eslint": "^7.21.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^5.1.3",
        "lint-staged": "^10.5.4",
        "typescript": "^4.2.3"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "engines": {
        "node": ">=14"
    }
}
