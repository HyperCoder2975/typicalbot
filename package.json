{
    "name": "typicalbot",
    "version": "4.0.1",
    "description": "TypicalBot is far from typical. It's stable, lightning fast, and easy to useâ€” TypicalBot will seamlessly help you moderate your server and provide entertainment for its members, no problem at all!",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "private": true,
    "scripts": {
        "build": "tsc",
        "build:clean": "git clean -d -x -e node_modules -e .env -f && tsc",
        "build:watch": "tsc --watch",
        "build:database": "node bin/setup.js",
        "build:instance": "tsc && node bin/pm2.js",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "start": "tsc && node dist/src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typicalbot/typicalbot.git"
    },
    "keywords": [
        "discordjs",
        "javascript",
        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/typicalbot/typicalbot/issues"
    },
    "homepage": "https://github.com/typicalbot/typicalbot#readme",
    "dependencies": {
        "@sentry/integrations": "^6.10.0",
        "@sentry/node": "^6.10.0",
        "discord.js": "github:discordjs/discord.js#35c2225f5035fce47b0defc12754bda5901a453c",
        "dotenv": "^10.0.0",
        "fs": "0.0.2",
        "i18next": "^20.3.5",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^9.4.4",
        "moment": "^2.29.1",
        "mongodb": "^3.6.10",
        "node-fetch": "^2.6.1",
        "pm2": "^5.1.0",
        "tslib": "^2.3.0",
        "vm2": "^3.9.3",
        "winston": "^3.3.3"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.3",
        "erlpack": "github:discord/erlpack",
        "utf-8-validate": "^5.0.5",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@types/i18next-node-fs-backend": "^2.1.1",
        "@types/klaw": "^3.0.2",
        "@types/mathjs": "^9.4.2",
        "@types/mongodb": "^3.6.20",
        "@types/node": "^14.17.6",
        "@types/node-fetch": "^2.5.12",
        "@types/ws": "^7.4.7",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "typescript": "^4.3.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "engines": {
        "node": ">=14"
    }
}
