    },
    "private": true,
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.0.3",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint-config-typicalbot": "^1.0.0",
        "lerna": "^3.16.4"
    },
    "eslintConfig": {
