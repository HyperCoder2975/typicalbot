{
    "version": "3.0.0-beta",
    "description": "TypicalBot is open-source discord bot written in JavaScript, using the Discord.JS library.",
    "license": "Apache-2.0",
    "main": "index.js",
    "scripts": {
        "lint": "eslint src scripts",
        "lint:fix": "eslint src scripts --fix",
        "build:database": "node ./scripts/setup.js",
        "build:instance": "node ./scripts/pm2.js",
        "build:docs": "esdoc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typicalbot/typicalbot.git"
    },
    "keywords": [
        "discordjs",
