        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/typicalbot/typicalbot/issues"
    },
    "homepage": "https://github.com/typicalbot/typicalbot#readme",
    "dependencies": {
        "bufferutil": "^4.0.1",
        "canvas": "github:Automattic/node-canvas",
        "canvas-constructor": "^1.1.2",
        "discord.js": "github:discordjs/discord.js#master",
        "dotenv": "^8.1.0",
        "erlpack": "github:discordapp/erlpack",
        "klaw": "^2.1.1",
        "mathjs": "^6.2.1",
        "moment": "^2.22.2",
        "node-fetch": "^2.6.0",
        "node-opus": "^0.3.2",
        "pm2": "^3.5.1",
        "rethinkdbdash": "^2.3.31",
        "simple-youtube-api": "github:hypercoder2975/simple-youtube-api",
        "simple-youtube-stream": "github:hypercoder2975/simple-youtube-stream",
        "utf-8-validate": "^5.0.2",
        "veza": "^0.6.0",
