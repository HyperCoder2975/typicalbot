{
    "name": "typicalbot",
    "version": "3.2.0",
    "description": "TypicalBot is open-source discord bot written in TypeScript, using the Discord.JS library.",
    "license": "Apache-2.0",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "build:database": "tsc && node dist/bin/setup.js",
        "build:instance": "tsc && node dist/bin/pm2.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "tsc && node dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sylkellc/typicalbot.git"
    },
    "keywords": [
        "discordjs",
        "javascript",
        "bot",
        "discord",
        "typicalbot",
        "discord-bot",
        "discordapp"
    ],
    "bugs": {
        "url": "https://github.com/sylkellc/typicalbot/issues"
    },
    "homepage": "https://github.com/sylkellc/typicalbot#readme",
    "dependencies": {
        "@sentry/node": "^5.15.4",
        "canvas": "^2.6.0",
        "canvas-constructor": "^3.2.0",
        "discord.js": "^12.1.1",
        "eslint-plugin-import": "^2.20.2",
        "fs": "0.0.2",
        "i18next": "^19.4.3",
        "i18next-node-fs-backend": "^2.1.3",
        "klaw": "^3.0.0",
        "mathjs": "^6.6.4",
        "moment": "^2.22.2",
        "node-fetch": "^2.6.0",
        "pm2": "^4.2.3",
        "rethinkdb-ts": "^2.4.2",
        "typescript": "^3.8.3",
        "veza": "^1.1.0",
        "vm2": "^3.9.1",
        "winston": "^3.2.1"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.1",
        "erlpack": "github:discordapp/erlpack",
        "utf-8-validate": "^5.0.2",
        "zlib-sync": "^0.1.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/i18next-node-fs-backend": "^2.1.0",
        "@types/klaw": "^3.0.1",
        "@types/mathjs": "^6.0.5",
        "@types/node": "^12.12.29",
        "@types/node-fetch": "^2.5.6",
        "@types/ws": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "all-contributors-cli": "^6.14.1",
        "eslint": "^6.1.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.1.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
